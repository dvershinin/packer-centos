{
  "builders": [
    {
      "accelerator": "kvm",
      "boot_command": [
        "<tab> text ks=http://{{ .HTTPIP }}:{{ .HTTPPort}}/ks6.cfg<enter>"
      ],
      "boot_wait": "10s",
      "disk_interface": "scsi",
      "disk_size": 5000,
      "format": "raw",
      "headless": true,
      "http_directory": "http",
      "iso_checksum": "3a9662cb65f9d59677d76acfdb73289da43b4599",
      "iso_checksum_type": "sha1",
      "iso_url": "http://mirror.yandex.ru/centos/6/isos/x86_64/CentOS-6.5-x86_64-netinstall.iso",
      "net_device": "virtio-net",
      "output_directory": "image/6/x86_64/qemu/",
      "qemuargs": [
        [
          "-m",
          "768m"
        ],
        [
          "-device",
          "virtio-scsi-pci,id=scsi0"
        ],
        [
          "-drive",
          "if=none,cache=unsafe,id=drive0,discard=unmap,file=image/6/x86_64/qemu/centos-6-x86_64.raw"
        ],
        [
          "-device",
          "scsi-hd,bus=scsi0.0,drive=drive0"
        ],
        [
          "-device",
          "virtio-net,netdev=user.0"
        ]
      ],
      "shutdown_command": "shutdown -h now",
      "ssh_password": "build",
      "ssh_username": "root",
      "ssh_wait_timeout": "10000s",
      "type": "qemu",
      "vm_name": "centos-6-x86_64"
    }
  ],
  "provisioners": [
    {
      "scripts": [
        "scripts/base.sh",
        "scripts/cleanup.sh"
      ],
      "type": "shell"
    }
  ]
}
